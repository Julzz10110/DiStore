{
  "http_port": 8080,
  "nodes": ["localhost:8081", "localhost:8082"],
  "replica_count": 2,
  "data_dir": "./test-data",
  "auth": {
    "enabled": false,
    "private_key": "<TEST_PRIVATE_KEY>",
    "public_key": "<TEST_PUBLIC_KEY>",
    "token_duration": 3600,
    "default_roles": ["read", "write"]
  },
  "tls": {
    "enabled": false,
    "cert_file": "cert.pem",
    "key_file": "key.pem"
  },
  "prometheus_port": 9090
}